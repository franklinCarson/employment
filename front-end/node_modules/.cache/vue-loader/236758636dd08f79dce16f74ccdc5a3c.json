"../components/JobOpportunityForm\";\n\nexport default {\n  name: 'Home',\n  components: {\n    UploadJobOppBtn,\n    JobListing,\n    JobOpportunityForm\n  },\n  data: () => ({\n    opportunities: []\n  }),\n  methods: {\n    showJobOpportunityForm: function () {\n      this.$refs.form.toggleShown()\n    },\n    getOpps: async function () {\n      try {\n        let res = await axios.get(\"/api/opportunity/pending\");\n        this.opportunities = res.data.opportunities;\n      } catch (e) {\n        console.log(e)\n      }\n    },\n    opportunityDeleted: function (id) {\n      this.opportunities = this.opportunities.filter((el) => el.id !== id)\n    }\n  },\n  created() {\n    this.getOpps();\n  }\n}\n</script>\n"]}]}