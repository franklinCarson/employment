ngOpportunities\",\n        props: {\n            admin:{\n                type: Boolean,\n                deleteItem: false\n            }\n        },\n        data: () => ({\n            opportunities: []\n        }),\n        methods: {\n            getOpps: async function () {\n                try {\n                    let res = await axios.get(\"/api/opportunity\");\n                    this.opportunities = res.data.opportunities;\n                } catch (e) {\n                    console.log(e)\n                }\n            },\n            removedOpportunity: function (id) {\n                this.opportunities = this.opportunities.filter((opportunity) => opportunity.id !== id);\n            }\n        },\n        created() {\n            this.getOpps()\n        }\n    }\n</script>\n\n<style scoped>\n\n</style>"]}]}